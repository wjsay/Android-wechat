<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/background_color">
    <!--标题-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48.0dp"
        android:background="@color/title_color">

        <ImageView
            android:id="@+id/myseting_back"
            android:layout_width="@dimen/backlen"
            android:layout_height="@dimen/backlen"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/back" />

        <TextView
            style="@style/head_title"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="30dp"
            android:text="设置" />

    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/white"
                android:layout_marginTop="@dimen/juli">
                <TextView
                    android:id="@+id/setting1"
                    android:background="@drawable/pressed_style"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height"
                    style="@style/contact_fonts_stayle"
                    android:text="新消息提醒"
                    android:onClick="clicksetting1"/>
                <View
                    android:layout_width="@dimen/line_length"
                    android:layout_height="@dimen/line_height"
                    android:layout_gravity="center"
                    android:background="@color/line_black">
                </View>
                <TextView
                    android:id="@+id/setting2"
                    android:background="@drawable/pressed_style"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height"
                    style="@style/contact_fonts_stayle"
                    android:text="勿扰模式"
                    android:onClick="clicksetting2"/>
                <View
                    android:layout_width="@dimen/line_length"
                    android:layout_height="@dimen/line_height"
                    android:layout_gravity="center"
                    android:background="@color/line_black">
                </View>
                <TextView
                    android:id="@+id/setting3"
                    android:background="@drawable/pressed_style"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height"
                    style="@style/contact_fonts_stayle"
                    android:text="聊天"
                    android:onClick="clicksetting3"/>
                <View
                    android:layout_width="@dimen/line_length"
                    android:layout_height="@dimen/line_height"
                    android:layout_gravity="center"
                    android:background="@color/line_black">
                </View>
                <TextView
                    android:id="@+id/setting4"
                    android:background="@drawable/pressed_style"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height"
                    style="@style/contact_fonts_stayle"
                    android:text="隐私"
                    android:onClick="clicksetting4"/>
                <View
                    android:layout_width="@dimen/line_length"
                    android:layout_height="@dimen/line_height"
                    android:layout_gravity="center"
                    android:background="@color/line_black">
                </View>
                <TextView
                    android:id="@+id/setting5"
                    android:background="@drawable/pressed_style"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height"
                    style="@style/contact_fonts_stayle"
                    android:text="通用"
                    android:onClick="clicksetting5"/>
                <View
                    android:layout_width="@dimen/line_length"
                    android:layout_height="@dimen/line_height"
                    android:layout_gravity="center"
                    android:background="@color/line_black">
                </View>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@drawable/pressed_style">
                    <TextView
                        android:id="@+id/setting6"
                        android:background="@color/white"
                        android:layout_width="260dp"
                        android:layout_height="@dimen/height"
                        android:drawablePadding="@dimen/height"
                        style="@style/contact_fonts_stayle"
                        android:text="账号与安全"
                        android:onClick="clicksetting6"/>
                    <ImageView
                        android:id="@+id/imageview1"
                        android:layout_width="100dp"
                        android:layout_height="35dp"
                        android:layout_gravity="center"
                        android:src="@drawable/baohu" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/juli"
                android:background="@color/white"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/setting7"
                    android:background="@drawable/pressed_style"
                    style="@style/contact_fonts_stayle"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height"
                    android:text="关于微信"
                    android:onClick="clicksetting7"/>
                <View
                    android:layout_width="@dimen/line_length"
                    android:layout_height="@dimen/line_height"
                    android:layout_gravity="center"
                    android:background="@color/line_black">
                </View>
                <TextView
                    android:onClick="clicksetting8"
                    android:id="@+id/setting8"
                    android:background="@drawable/pressed_style"
                    style="@style/contact_fonts_stayle"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height"
                    android:text="帮助和反馈" />
            </LinearLayout>
            <LinearLayout
                android:onClick="clicksetting9"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/juli"
                android:background="@color/white"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/setting9"
                    android:background="@drawable/pressed_style"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/height"
                    style="@style/contact_fonts_stayle"
                    android:text="插件" />
                <ImageView
                    android:id="@+id/imageview2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/lighting"/>
            </LinearLayout>
            <TextView
                android:id="@+id/setting10"
                android:background="@drawable/pressed_style"
                android:layout_marginTop="@dimen/juli"
                android:layout_marginBottom="30dp"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/height"
                style="@style/contact_fonts_stayle"
                android:text="退出"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
